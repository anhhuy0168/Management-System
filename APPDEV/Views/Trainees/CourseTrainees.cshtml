@model IEnumerable<APPDEV.Models.Trainee>
@{
    ViewBag.Title = "CourseTrainees";
}

<h2>trainees in this Course</h2>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Trainees</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.FullName</td>
                <td>@item.User.Email</td>
            </tr>
        }
    </tbody>
</table>